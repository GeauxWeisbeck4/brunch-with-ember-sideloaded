{
	"name": "brunch-with-ember-sideloaded",
	"repo": "ksnyde/brunch-with-ember-sideloaded",
	"version": "0.0.4",
	"ignore": [
		"**/.*",
		"node_modules",
		"components"
	],
	"dependencies": {
		"modernizr": "2.*",
		"normalize-css":"2.*",
		"console-polyfill": "~0.1.0",
		"jquery": "2.0.*",
		"ember": "1.*",
		"bootstrap": "~3.0.*",
		"handlebars": "1.*",
		"ember-data-shim": "1.*",
		"ember-addons.bs_for_ember": "*",
		"font-awesome": "4.*",
		"swag": "0.*",
		"moment": "~2.*"
	},
	"overrides": {
		"modernizr": {
			"main":"modernizr.js",
			"dependencies":{}
		},
		"jquery": {
			"main": "jquery.js"
		},
		"bootstrap": {
			"main": [
				"dist/js/bootstrap.js",
				"dist/css/bootstrap.css",
				"dist/fonts/glyphicons-halflings-regular.woff",
				"dist/fonts/glyphicons-halflings-regular.ttf",
				"dist/fonts/glyphicons-halflings-regular.svg"	
			],
			"dependencies": {
				"jquery": "~1.9.1"
			}
		},
		"ember-addons.bs_for_ember": {
			"main": [
				"dist/js/bs-core.max.js",
				"dist/js/bs-alert.max.js",
				"dist/js/bs-badge.max.js",
				"dist/js/bs-basic.max.js",
				"dist/js/bs-button.max.js",
				"dist/js/bs-label.max.js",
				"dist/js/bs-modal.max.js",
				"dist/js/bs-nav.max.js",
				"dist/js/bs-notifications.max.js",
				"dist/js/bs-progressbar.max.js",
				"dist/js/bs-wizard.max.js"
			],
			"dependencies": {
				"bootstrap": "3.0.0",
				"ember": "1.0.0",
				"handlebars": "1.0.0"
			}
		},
		"font-awesome":{
			"main": "css/font-awesome.css"
		},
		"moment": {
			"main": "moment.js"
		},
		"resolutions": {
			"bootstrap": "~3.0.0"
		}
	}
}
